<?xml version="1.0"?>
<extension key="oddc" type="module">
  <file>oddc</file>
  <name>openDemocracy Donation CiviCRM Extension</name>
  <description>Custom code for donations forms.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/oddc</url>
    <url desc="Documentation">https://github.com/artfulrobot/oddc</url>
    <url desc="Support">https://github.com/artfulrobot/oddc</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-12-03</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a custom module not of any use to anyone else.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
